{
  "version": 2,
  "buildCommand": "node -e \"try{require('fs').chmodSync('./vercel-build.sh', 0o755)}catch(e){console.error(e)}\" && ./vercel-build.sh",
  "installCommand": "npm install && npm install typescript@5.8.3 @types/react@19.1.2 @types/node@22.15.3 @types/react-dom@19.1.2",
  "framework": "nextjs",
  "devCommand": "npm run dev",
  "outputDirectory": ".next",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://surajkumaar-clarirai.hf.space",
    "NEXT_PUBLIC_ENVIRONMENT": "production",
    "NEXT_TYPESCRIPT_IGNORE_ERRORS": "true",
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
